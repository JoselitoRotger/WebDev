import makeItWork from "./makeitwork.js";

var myWackyArray = [
  [1, 2, 3, 4, 5, 6, "declarative", 8, 9, 10],
  [1, "environments", 3, 4, 5, 6, 7, 8, 9, 10],
  [1, 2, 3, 4, 5, 6, 7, 8, "functional", 10],
  [1, 2, "interpreted", 3, 4, 5, 6, 7, 8, 9, 10],
  [1, 2, 3, "JavaScript", 4, 5, 6, 7, 8, 9, 10],
  [1, 2, "lightweight", 4, 5, 6, 7, "language", 9, 10],
  [1, 2, 3, 4, "programming", 6, 7, 8, 9, 10],
  [1, 2, 3, 4, 5, "supporting", 7, 8, 9, 10],
 ];

makeItWork(
  //"supporting"
  myWackyArray[7][5],
  //"declarative"
  myWackyArray[0][6],
  //"environments"
  myWackyArray[1][1],
  //"JavaScript"
  myWackyArray[4][3],
  //"language"
  myWackyArray[5][7],
  //"programming"
  myWackyArray[6][4],
  //"functional"
  myWackyArray[2][8],
  //"interpreted"
  myWackyArray[3][2],
 //"lightweight"
  myWackyArray[5][2]
);
